<% @articles.each do |date, articles| %>
  <div class="each_day">
    <h2><%= date.to_date %></h2>
    <% articles.each do |article| %>
      <div>
        <%= link_to article.title, article.url, target: '_blank' %>
        <%= render partial: 'article_like_box', locals: { article: article } %>
      </div>
    <% end %>
  </div>
<% end %>
